<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zl.sp.persistence.mapper.OrderMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.zl.sp.persistence.entity.Order">
		<id column="order_id" property="orderId" />
		<result column="order_user_name" property="orderUserName" />
		<result column="oder_subject" property="oderSubject" />
		<result column="order_amount" property="orderAmount" />
		<result column="order_status" property="orderStatus" />
		<result column="order_out_trade_no" property="orderOutTradeNo" />
		<result column="order_generate_time" property="orderGenerateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        order_id AS orderId, order_user_name AS orderUserName, oder_subject AS oderSubject, order_amount AS orderAmount, order_status AS orderStatus, order_out_trade_no AS orderOutTradeNo, order_generate_time AS orderGenerateTime
    </sql>

</mapper>
